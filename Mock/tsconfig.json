{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "typeRoots" : ["node_modules/@types", "mock/.expo/types", "mock/declaration.d.ts"],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "/*.ts",
    "/*.tsx",
    "*.png",
    "*.d.ts",
    "src",
    
    ".expo/types//*.ts",
    "expo-env.d.ts",
    "app/App.js",
    ".expo/types/**/*.ts"
  ]
}
